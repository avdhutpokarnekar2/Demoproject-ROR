<h1>Order details for user</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">name</th>
      <th scope="col">price Rs.</th>
      <th scope="col">quantity</th>
    </tr>
  </thead>
  <tbody>
    User order id :- <%= @order.id %>
    order status :- <%= @order.status %>
    <% @order.products.each do |product| %>
    <tr>
      <td><%= product.name %>
      <td><%= product.price %></td>
      <td><%= product.quantity %></td>
    <% end %>
    </tr>
  </tbody>
</table>